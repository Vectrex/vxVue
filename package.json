    "jsdom": "^27.0.1",
    "vite": "^7.1.10",
    "eslint": "^9.38.0",
    "eslint-plugin-vue": "^9.33.0",
    "@eslint/js": "^9.38.0",
    "@stylistic/eslint-plugin": "^5.5.0",
